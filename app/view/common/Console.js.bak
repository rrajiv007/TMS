	storeFilter:function(tmpStore,cnt,tmpPanel)
	{		
		var me =this;
		var flag=true;
		var filterTransactionBtn=[]	
		var tmpcnt=cnt;
		var tmpcnt1=cnt;
		var tmpSection = Ext.create("Ext.container.Container",
						{
							"layout":{
									type: 'hbox',
									align: 'middle',
									pack: 'center',
								},
							height:40,
							width:40,
							cls:'console_navigation'
						})
		tmpStore.clearFilter();
		tmpStore.filterBy(function(record, id)
		{				
			if(record.get("SLNO") <=cnt)
			{
				if (flag)
				{
					//console.log(record.get("SLNO"),tmpcnt,"Rajiv")
					flag=false;
					tmpcnt=tmpcnt+cnt;
					filterTransactionBtn.push(
						{xtype:"image",align:"right",src: 'resources/images/common/spacer.png', cls:'console_navigationBTN',
						listeners: {
							el: {
								click: function() 
						{
						
						tmpStore.clearFilter();		
						tmpStore.filterBy(function(record, id)
						{			
							if(record.get("SLNO") <=cnt)
							{
								return true;
							}
						}, this);
						}
						}}						
						})
				}
				return true;
			}
			else
			{	
			//console.log(record.get("SLNO"),tmpcnt,"Rajiv1")
			if(record.get("SLNO") <= tmpcnt)
			{	
				tmpcnt1=tmpcnt;
				if (!flag)
				{						
					var cnt1=record.get("SLNO")					
					flag=true;
					filterTransactionBtn.push(
						{xtype:"image", align:"right", src:'resources/images/common/spacer.png', cls:'console_navigationBTN',						
						listeners: {
							el: {
								click: function() 
									{	
									//me.filterStore(tmpcnt,cnt1);
									//console.log(record.get("SLNO"),tmpcnt,cnt1,cnt1+cnt,"Rajiv1")						
									tmpStore.clearFilter();		
									tmpStore.filterBy(function(record, id)
									{		
										
										if(record.get("SLNO") >=cnt1 & record.get("SLNO") <cnt1+cnt)
										{
											return true;
										}
									}, this);
									}
						}
						}						
						})
				}
			}
			if(record.get("SLNO") == tmpcnt)
			{
			//console.log("Rajiv2")
			tmpcnt=tmpcnt+cnt;
			flag=false;
			}				
			}
		}, this);
		
		if 	(tmpcnt1>cnt)
		{
		tmpSection.add(filterTransactionBtn)
		tmpPanel.add(tmpSection);
		}
	},
	loadEntityScreens:function(tmp_searchScreen,tmp_appScreen,tmp_reportScreen,tmp_dashboardScreen,tmp_initialValues)/*code added for id: 57410*/
	{
		if(tmp_searchScreen=="" & tmp_appScreen == "" & tmp_reportScreen=="")
		{
			return
		}

		if(tmp_searchScreen=="common.ivms")
		{
			var tmpHTMLStr =ivmsPath+ Math.random();
			if (newIVMSPtr != undefined && typeof newIVMSPtr.close != undefined) 
				newIVMSPtr.close();
				
			newIVMSPtr = window.open(tmpHTMLStr, 'cuetransivms');
			return
		}

		
		if(tmp_searchScreen=="")
		{
			plf.viewport.ptrContentPanel.searchScreenJS = "";
		}
		else
		{
			plf.viewport.ptrContentPanel.searchScreenJS = "CueTrans.view."+tmp_searchScreen;
		}
		
		if(tmp_appScreen=="")
		{
			plf.viewport.ptrContentPanel.appScreenJS = "";
		}
		else
		{
			plf.viewport.ptrContentPanel.appScreenJS = "CueTrans.view."+tmp_appScreen;
		}
		
		if(tmp_reportScreen == "")
		{
			plf.viewport.ptrContentPanel.reportScreenJS = "";
		}
		else
		{
			plf.viewport.ptrContentPanel.reportScreenJS = "CueTrans.view."+tmp_reportScreen;
		}
		/*code added for id: 57410 starts here*/
		if(tmp_dashboardScreen == "")
		{
			plf.viewport.ptrContentPanel.dashboardJS = "";
		}
		else
		{
			plf.viewport.ptrContentPanel.dashboardJS = "CueTrans.view."+tmp_dashboardScreen;
		}
		/*code added for id: 57410 ends here*/
		
		plf.viewport.ptrContentPanel.searchScreenPtr.removeAll();
		plf.viewport.ptrContentPanel.appScreenPtr.removeAll();
		plf.viewport.ptrContentPanel.reportScreenPtr.removeAll();
		//plf.viewport.ptrContentPanel.reportResultPtr.removeAll();
		plf.viewport.ptrContentPanel.dashboardPtr.removeAll();/*code added for id: 57410*/
		
		/*if(tmp_dashboardScreen != "")
		{
			plf.viewport.ptrContentPanel.loadDashboardScreen(tmp_initialValues);
		}
		else */if(tmp_searchScreen=="" & tmp_appScreen != "")
		{
			plf.viewport.ptrContentPanel.loadAppScreen(tmp_initialValues);
		}
		else if(tmp_searchScreen!="")
